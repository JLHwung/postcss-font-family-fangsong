{
  "name": "postcss-font-family-fangsong",
  "version": "2.0.0",
  "description": "Use the fangsong font family in CSS",
  "author": "Jùnliàng Huáng<jlhwung@gmail.com>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JLHwung/postcss-font-family-fangsong.git"
  },
  "homepage": "https://github.com/JLHwung/postcss-font-family-fangsong#readme",
  "bugs": "https://github.com/JLHwung/postcss-font-family-fangsong/issues",
  "main": "./dist/index.bundle.cjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.bundle.mjs",
    "require": "./dist/index.bundle.cjs"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rollup -c .rollup.mjs",
    "check:js": "eslint *.[cm]js --cache --ignore-pattern .gitignore --quiet",
    "prepublishOnly": "yarn test",
    "check": "yarn check:js",
    "test": "yarn build && echo 'Running tests...'; yarn check && yarn test:tape",
    "test:tape": "postcss-tape --config .tape.cjs"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/eslint-parser": "7.28.6",
    "@babel/preset-env": "7.29.0",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@rollup/plugin-babel": "6.1.0",
    "eslint": "9.39.2",
    "eslint-config-dev": "3.3.1",
    "postcss": "8.5.6",
    "postcss-tape": "6.0.1",
    "pre-commit": "1.2.2",
    "rollup": "4.57.1"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "eslintConfig": {
    "extends": "dev",
    "parser": "@babel/eslint-parser"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "ua",
    "user",
    "agent",
    "platform",
    "fangsong",
    "default",
    "font",
    "font-family",
    "family",
    "face",
    "type",
    "typeface"
  ],
  "yaspeller": {
    "dictionary": [
      "Affirmer",
      "Affirmer's",
      "babili",
      "babelrc",
      "browserslist",
      "caniuse",
      "devDependencies",
      "env",
      "es6",
      "esm",
      "fangsong",
      "Gulpfile",
      "Gruntfile",
      "istanbul",
      "js",
      "lockfile",
      "npm",
      "nyc",
      "postcss",
      "PostCSS",
      "polyfill",
      "Refactors",
      "rollup",
      "sublicensable",
      "ui",
      "yaspeller",
      "^[A-f0-9]{8}$"
    ]
  }
}
